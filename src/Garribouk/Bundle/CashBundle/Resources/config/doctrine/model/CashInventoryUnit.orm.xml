<?xml version="1.0" encoding="UTF-8" ?>

<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                                      http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Garribouk\Bundle\CashBundle\Entity\CashInventoryUnit" table="cash_inventory_unit">
        <many-to-one field="note" target-entity="Garribouk\Bundle\CashBundle\Entity\NoteInterface" inversed-by="inventoryUnits">
            <join-column name="note_id" referenced-column-name="id" nullable="false" />
        </many-to-one>
    </entity>

</doctrine-mapping>